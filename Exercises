

import java.util.*;

import static java.lang.Integer.sum;

public class Main {
    public static void main(String args[]) {
        int[] arr = {1, 6, 16, 3};

        {
            int sum = 0;
            int i;
            for (i = 0; i < arr.length; i++) {
                sum += arr[i];

            }
            System.out.println("The sum is:" + sum);


        }
    }
}
